#> 떼껄룩 코인 확인
function checkCoinAmount(p: player, require: integer, coin: text) :: boolean:
    set {_amount} to 0
    loop items in {_p}'s inventory:
        set {_tag} to nbt compound of loop-item
        "%{_tag}%" contains "CustomFishing"
        "%{_tag}%" does not contain "shulker"
        if {_coin} is "초심자 코인":
            "%{_tag}%" does not contain "rainbow_fish"
            "%{_tag}%" does not contain "crab"
            "%{_tag}%" does not contain "silver_star"
            "%{_tag}%" does not contain "golden_star"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_coin} is "은빛 코인":
            "%{_tag}%" contains "silver_star"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_coin} is "금빛 코인":
            "%{_tag}%" contains "golden_star"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_coin} is "별빛 코인":
            if "%{_tag}%" contains "rainbow_fish":
                add "%item amount of loop-item%" parsed as integer to {_amount}
            if "%{_tag}%" contains "crab":
                add "%item amount of loop-item%" parsed as integer to {_amount}
    if {_amount} >= {_require}:
        return true
    else:
        return false
#> @param p 플레이어
#> @param c 코인 종류 : 나중에 어느 물고기 검사할지 정함
#> @param 
#> @return true : 교환 가능 / false : 교환 불가능
function checkCoinEvent(p: player, c: text) :: boolean:
    if {_c} is "초심자 코인":
        return checkCoinAmount({_p}, {@COIN_TIER_1_REQUIRE}, {_c})
    else if {_c} is "은빛 코인":
        return checkCoinAmount({_p}, {@COIN_TIER_2_REQUIRE}, {_c})
    else if {_c} is "금빛 코인":
        return checkCoinAmount({_p}, {@COIN_TIER_3_REQUIRE}, {_c})
    else if {_c} is "별빛 코인":
        return checkCoinAmount({_p}, {@COIN_TIER_4_REQUIRE}, {_c})
    return false

