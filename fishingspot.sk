    if "%{_nbt}%" contains "Slot":
        set {_temp} to true
        message "ㅎㅇ" to player
    else:
        cancel event



on break:
    player's world is {@WORLD}
    player is not op
    event-block is not barrel
    cancel event

on place:
    player's world is {@WORLD}
    player is not op
    cancel event

#> 블럭 우클릭(문이나 다락문, 레버 등등)

on rightclick:
    player's world is {@WORLD}
    player is not op
    event-entity is set
    if event-entity is horse:
        cancel event
    else if "%name of event-entity%" contains "boat":
        cancel event

on rightclick:
    player's world is {@WORLD}
    player is not op
    if "%player's tool%" contains "bucket":
        cancel event
    else if "%player's tool%" contains "양동이":
        cancel event



on rightclick:
    player's world is {@WORLD}
    player is not op
    event-block is set
    if "%event-block%" contains "carpet":
        set {_index} to true
    else if "%event-block%" contains "flower" and "pot":
        set {_index} to true
    else if "%event-block%" contains "slab":
        set {_index} to true
    else if "%event-block%" contains "stair":
        set {_index} to true
    else if "%event-block%" contains "ender" and "chest":
        set {_index} to true
    if {_index} is not set:
        cancel event
        if "%region at player%" contains "fishingspotkinggrave":
            "%event-block%" contains "trapped" and "chest":
