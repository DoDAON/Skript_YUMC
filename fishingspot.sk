function checkShop1Amount(p: player, require: integer, rod: text) :: boolean:
    set {_amount} to 0
    loop items in {_p}'s inventory:
        set {_tag} to nbt compound of loop-item
        if {_rod} is "초심자의 낚싯대":
            "%{_tag}%" contains "초심자 코인"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_rod} is "은빛 낚싯대":
            "%{_tag}%" contains "은빛 코인"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_rod} is "금빛 낚싯대":
            "%{_tag}%" contains "금빛 코인"
            add "%item amount of loop-item%" parsed as integer to {_amount}
        else if {_rod} is "별빛 낚싯대":
            "%{_tag}%" contains "별빛 코인"
            add "%item amount of loop-item%" parsed as integer to {_amount}
    if {_amount} >= {_require}:
        return true
    else:
        return false

function checkShop1Event(p: player, rod: text) :: boolean:
    if {_rod} is "초심자의 낚싯대":
        return checkShop1Amount({_p}, {@SHOP1_TIER_1_REQUIRE}, {_rod})
    else if {_rod} is "은빛 낚싯대":
        return checkShop1Amount({_p}, {@SHOP1_TIER_2_REQUIRE}, {_rod})
    else if {_rod} is "금빛 낚싯대":
        return checkShop1Amount({_p}, {@SHOP1_TIER_3_REQUIRE}, {_rod})
    else if {_rod} is "별빛 낚싯대":
        return checkShop1Amount({_p}, {@SHOP1_TIER_4_REQUIRE}, {_rod})
    return false


