    set {_questItem4} to 0
    loop items in {_p}'s inventory:
        if loop-item is fishing rod:
            set {_tag} to nbt compound of loop-item
            "%{_tag}%" does not contain "shulker"
            if "%{_tag}%" contains "beginner_rod":
                set {_dd} to "%item amount of loop-item%" parsed as integer
                add {_dd} to {_questItem1}
            else if "%{_tag}%" contains "silver_rod":
                set {_dd} to "%item amount of loop-item%" parsed as integer
                add {_dd} to {_questItem2}
            else if "%{_tag}%" contains "golden_rod":
                set {_dd} to "%item amount of loop-item%" parsed as integer
                add {_dd} to {_questItem3}
            else if "%{_tag}%" contains "star_rod":
                set {_dd} to "%item amount of loop-item%" parsed as integer
                add {_dd} to {_questItem4}
        set {_dd} to 0
    set {_checkSum} to 0
    if {_questItem1} >= {@MASTER_QUEST_1}:
        add 1 to {_checkSum}
    if {_questItem2} >= {@MASTER_QUEST_2}:
        add 1 to {_checkSum}
    if {_questItem3} >= {@MASTER_QUEST_3}:
        add 1 to {_checkSum}
    if {_questItem4} >= {@MASTER_QUEST_4}:
        add 1 to {_checkSum}
    if {_checkSum} is {@MASTER_QUEST_LIST}:
        return true
    return false


function clearMasterEvent(p: player):
    loop items in {_p}'s inventory:
        if loop-item is fishing rod:
            set {_tag} to nbt compound of loop-item
            if "%{_tag}%" contains "beginner_rod":
                remove {@MASTER_QUEST_1} of loop-item from {_p}'s inventory
            else if "%{_tag}%" contains "silver_rod":
                remove {@MASTER_QUEST_2} of loop-item from {_p}'s inventory
            else if "%{_tag}%" contains "golden_rod":
                remove {@MASTER_QUEST_3} of loop-item from {_p}'s inventory
            else if "%{_tag}%" contains "star_rod":
                remove {@MASTER_QUEST_4} of loop-item from {_p}'s inventory


