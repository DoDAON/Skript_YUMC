#> 퀘스트 시간 설정
function questClearTime() :: text:
    set {_time} to now formatted as "HH:mm"
    set {_date} to now formatted as "yyyy/MM/dd"
    set {_result} to "%{_date}% %{_time}%"
    return {_result}





#> 꿀꿀이 교환 검사 로직
#> 인벤에 재료 하나하나 검사해서 맞는지 판단
function checkLavaEvent(p: player) :: boolean:
    set {_up} to uuid of {_p}
    #> 비기너 낚싯대
    set {_questItem1} to 0
    #> 금주괴
    set {_questItem2} to 0
    #> 복어가 담긴 양동이
    set {_questItem3} to 0
    #> 뼈다귀
    set {_questItem4} to 0
    loop items in {_p}'s inventory:
        if loop-item is fishing rod:
            set {_tag} to nbt compound of loop-item
            if "%{_tag}%" contains "beginner_rod":
                set {_dd} to "%item amount of loop-item%" parsed as integer
            add {_dd} to {_questItem1}
        else if loop-item is gold ingot:
            set {_dd} to "%item amount of loop-item%" parsed as integer
            add {_dd} to {_questItem2}
        else if loop-item is pufferfish bucket:
            #> 뒤에 아쿠아틱 몹 바꿔야할듯 정확한 이름으로
            set {_dd} to "%item amount of loop-item%" parsed as integer
            add {_dd} to {_questItem3}
        else if loop-item is bone:
            set {_dd} to "%item amount of loop-item%" parsed as integer
            add {_dd} to {_questItem4}
        set {_dd} to 0
    set {_checkSum} to 0
    if {_questItem1} >= {@LAVA_QUEST_1}:
        add 1 to {_checkSum}
    if {_questItem2} >= {@LAVA_QUEST_2}:
        add 1 to {_checkSum}
    if {_questItem3} >= {@LAVA_QUEST_3}:
        add 1 to {_checkSum}
    if {_questItem4} >= {@LAVA_QUEST_4}:
        add 1 to {_checkSum}
    if {_checkSum} is {@LAVA_QUEST_LIST}:
        return true
    return false

function clearLavaEvent(p: player):
    set {_delete1} to false
    set {_delete2} to {@LAVA_QUEST_2}
    set {_delete3} to false
    set {_delete4} to {@LAVA_QUEST_4}
    loop items in {_p}'s inventory:
        if loop-item is fishing rod:
            set {_tag} to nbt compound of loop-item
            "%{_tag}%" does not contain "shulker"
            if "%{_tag}%" contains "beginner_rod":
                {_delete1} is false
                remove {@LAVA_QUEST_1} of loop-item from {_p}'s inventory
                set {_delete1} to true
        else if loop-item is gold ingot:
            {_delete2} is not 0
            set {_dd} to "%item amount of loop-item%" parsed as integer
            if {_dd} > {_delete2}:
                set {_dd} to {_delete2}
            remove {_dd} of loop-item from {_p}'s inventory
            subtract {_dd} from {_delete2}
        else if loop-item is pufferfish bucket:
            {_delete3} is false
            remove {@LAVA_QUEST_3} of loop-item from {_p}'s inventory
            set {_delete3} to true
        else if loop-item is bone:
            {_delete4} is not 0
            set {_dd} to "%item amount of loop-item%" parsed as integer
            if {_dd} > {_delete4}:
                set {_dd} to {_delete4}
            remove {_dd} of loop-item from {_p}'s inventory
            subtract {_dd} from {_delete4}

